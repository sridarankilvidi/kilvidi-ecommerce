{
    "version": 2,
    "name": "kilvidi-ecommerce",
    "builds": [
        {
            "src": "./api/products/index.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    {
        "framework": "nextjs"
    }
}